@prefix : <http://waysysweb.com/ontolgoy/lootedart#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://waysysweb.com/ontolgoy/lootedart#> .

<http://waysysweb.com/ontolgoy/lootedart#> rdf:type owl:Ontology ;
                                            rdfs:comment "This ontology tracks information about art looted by the Nazis during World War II.  It also describes what happened to the art after the war." ;
                                            rdfs:seeAlso "Jonathan Petropoulos, Goring's Man in Paris, ISBN: 0300251920" ;
                                            owl:versionInfo "31-Jan-2021" .

#################################################################
#    Object Properties
#################################################################

###  http://waysysweb.com/ontolgoy/lootedart#agentFor
:agentFor rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :participant ;
          rdfs:range :Party ;
          rdfs:comment "Indicates the party for whom the subject is an agent." .


###  http://waysysweb.com/ontolgoy/lootedart#atDate
:atDate rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :hasTime ;
        rdf:type owl:FunctionalProperty ;
        owl:propertyDisjointWith :ending ;
        rdfs:comment "This property represents the point in time of an interaction." .


###  http://waysysweb.com/ontolgoy/lootedart#auctioneer
:auctioneer rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :participant ;
            rdfs:range :Person .


###  http://waysysweb.com/ontolgoy/lootedart#beginning
:beginning rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasTime ;
           rdf:type owl:FunctionalProperty ;
           owl:propertyDisjointWith :ending ;
           rdfs:comment "The beginning of a duration." .


###  http://waysysweb.com/ontolgoy/lootedart#buyer
:buyer rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :participant ;
       rdfs:range :Party .


###  http://waysysweb.com/ontolgoy/lootedart#child
:child rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :family ;
       owl:inverseOf :parent .


###  http://waysysweb.com/ontolgoy/lootedart#city
:city rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf :located ;
      rdf:type owl:FunctionalProperty ;
      rdfs:range :City ;
      rdfs:comment "The city where a thing is located." .


###  http://waysysweb.com/ontolgoy/lootedart#conducted
:conducted rdf:type owl:ObjectProperty ;
           owl:inverseOf :conductedBy ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Event
                                      :Operation
                                    )
                      ] ;
           rdfs:comment "A party conducted and operation" .


###  http://waysysweb.com/ontolgoy/lootedart#conductedBy
:conductedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:FunctionalProperty ;
             rdfs:range :Party ;
             rdfs:comment "A party operation is conducted by a party." .


###  http://waysysweb.com/ontolgoy/lootedart#conductedTheft
:conductedTheft rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty .


###  http://waysysweb.com/ontolgoy/lootedart#country
:country rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :located ;
         rdf:type owl:FunctionalProperty ;
         rdfs:range :Country ;
         rdfs:comment "The country where a thing is located." .


###  http://waysysweb.com/ontolgoy/lootedart#created
:created rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         owl:inverseOf :creator ;
         rdfs:range [ rdf:type owl:Restriction ;
                      owl:onProperty :creator ;
                      owl:someValuesFrom :ArtWork
                    ] ;
         rdfs:comment "A person created a work of art." .


###  http://waysysweb.com/ontolgoy/lootedart#creator
:creator rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:range [ rdf:type owl:Restriction ;
                      owl:onProperty :creator ;
                      owl:someValuesFrom :Person
                    ] .


###  http://waysysweb.com/ontolgoy/lootedart#employed
:employed rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          owl:inverseOf :employedBy .


###  http://waysysweb.com/ontolgoy/lootedart#employedBy
:employedBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :participant ;
            rdfs:range :Party ;
            rdfs:comment "Reference from an employment to the employer." .


###  http://waysysweb.com/ontolgoy/lootedart#ending
:ending rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :hasTime ;
        rdf:type owl:FunctionalProperty ;
        rdfs:comment "The end of a duration." .


###  http://waysysweb.com/ontolgoy/lootedart#family
:family rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty .


###  http://waysysweb.com/ontolgoy/lootedart#founded
:founded rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :participant ;
         rdfs:range :Organization ;
         rdfs:comment "A party founds an organization." .


###  http://waysysweb.com/ontolgoy/lootedart#hasDuration
:hasDuration rdf:type owl:ObjectProperty ;
             rdfs:range :Duration ;
             rdfs:comment "HasDuration specifies a duration during which an interaction is in effect." .


###  http://waysysweb.com/ontolgoy/lootedart#hasEmployment
:hasEmployment rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:range :Employment ;
               rdfs:comment "The reference to an employment." .


###  http://waysysweb.com/ontolgoy/lootedart#hasOwnership
:hasOwnership rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:range :Ownership ;
              rdfs:comment "A party has an ownerhip relation." .


###  http://waysysweb.com/ontolgoy/lootedart#hasParts
:hasParts rdf:type owl:ObjectProperty ;
          owl:inverseOf :partOf ;
          rdfs:range :Organization ;
          rdfs:comment "An organization may have subsidiaries, divisions, or other parts." .


###  http://waysysweb.com/ontolgoy/lootedart#hasTime
:hasTime rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:range :PointInTime ;
         rdfs:comment "This property is a category of properties that represent a point in time." .


###  http://waysysweb.com/ontolgoy/lootedart#knownFor
:knownFor rdf:type owl:ObjectProperty ;
          rdfs:comment "A thing is known for another thing." .


###  http://waysysweb.com/ontolgoy/lootedart#leader
:leader rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :participant ;
        rdf:type owl:FunctionalProperty ;
        rdfs:range :Person ;
        rdfs:comment "The person who directs an interaction or event." .


###  http://waysysweb.com/ontolgoy/lootedart#located
:located rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdf:type owl:FunctionalProperty ;
         rdfs:comment "A class of properties related to where a thing is.  This is functional since a thing cannot be in two places at once." .


###  http://waysysweb.com/ontolgoy/lootedart#memberOf
:memberOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :participant ;
          rdfs:range :Party ;
          rdfs:comment "A person is a member of an organization." .


###  http://waysysweb.com/ontolgoy/lootedart#nationality
:nationality rdf:type owl:ObjectProperty ;
             rdfs:range :Country ;
             rdfs:comment "The nationality of a thing is the country of origin.  For a person, it is the country of birth.  For an organizatioin, it is the country where the organization is founded.  For an art work, it is the country where it was created." .


###  http://waysysweb.com/ontolgoy/lootedart#occupation
:occupation rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:range :Occupation ;
            rdfs:comment "The occupation describes the type of work a person performs.  An occupation may carry a duration." .


###  http://waysysweb.com/ontolgoy/lootedart#owner
:owner rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :participant ;
       rdfs:range :Party ;
       rdfs:comment "The party that owns something." .


###  http://waysysweb.com/ontolgoy/lootedart#owns
:owns rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      rdfs:range :ArtWork ;
      rdfs:comment "The act, state, or right of possessing a work of art.." .


###  http://waysysweb.com/ontolgoy/lootedart#parent
:parent rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :family .


###  http://waysysweb.com/ontolgoy/lootedart#partOf
:partOf rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdf:type owl:TransitiveProperty ;
        rdfs:range :Organization ;
        rdfs:comment "An organization is part of another organization." .


###  http://waysysweb.com/ontolgoy/lootedart#participant
:participant rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:comment "A person who particpates in an event." .


###  http://waysysweb.com/ontolgoy/lootedart#partner
:partner rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :participant ;
         rdfs:range :Person .


###  http://waysysweb.com/ontolgoy/lootedart#perpetrator
:perpetrator rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :participant ;
             rdfs:range :Party ;
             rdfs:comment "A perpetrator conducts a theft." .


###  http://waysysweb.com/ontolgoy/lootedart#propertyDisposition
:propertyDisposition rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty .


###  http://waysysweb.com/ontolgoy/lootedart#reportedTo
:reportedTo rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:range :Person ;
            rdfs:comment "The person to whom a person reports as part of employment." .


###  http://waysysweb.com/ontolgoy/lootedart#role
:role rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      rdfs:range :Role ;
      rdfs:comment "Role is the position a person occupies in an employment." .


###  http://waysysweb.com/ontolgoy/lootedart#sales
:sales rdf:type owl:ObjectProperty ;
       rdfs:range :Sale ;
       rdfs:comment "Sales made at an auction." .


###  http://waysysweb.com/ontolgoy/lootedart#seller
:seller rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :participant ;
        rdfs:range :Party ;
        rdfs:comment "A seller is a party that particpates in a transaction by relinquishing ownership of an art work." .


###  http://waysysweb.com/ontolgoy/lootedart#spouse
:spouse rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :family ;
        rdf:type owl:SymmetricProperty ;
        rdfs:range :Person ;
        rdfs:comment "The person married to a person." .


###  http://waysysweb.com/ontolgoy/lootedart#started
:started rdf:type owl:ObjectProperty ;
         owl:inverseOf :startedBy ;
         rdfs:range :ArtCollection ;
         rdfs:comment "A party starts an art collection." .


###  http://waysysweb.com/ontolgoy/lootedart#startedBy
:startedBy rdf:type owl:ObjectProperty ;
           rdfs:comment "An art collection is started by a party." .


###  http://waysysweb.com/ontolgoy/lootedart#thingOffered
:thingOffered rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :propertyDisposition ;
              rdfs:range :ArtWork ;
              rdfs:comment "An item offered for sale." .


###  http://waysysweb.com/ontolgoy/lootedart#thingSold
:thingSold rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :propertyDisposition ;
           rdfs:range :ArtWork ;
           rdfs:comment "The thing sold is an art work." .


###  http://waysysweb.com/ontolgoy/lootedart#thingStolen
:thingStolen rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :propertyDisposition ;
             rdfs:range :ArtCollection .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdfs:comment "" ,
                                   "A party started an art collection." .


#################################################################
#    Data properties
#################################################################

###  http://waysysweb.com/ontolgoy/lootedart#alternateName
:alternateName rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:range xsd:string .


###  http://waysysweb.com/ontolgoy/lootedart#month
:month rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       rdfs:range [ rdf:type rdfs:Datatype ;
                    owl:onDatatype xsd:positiveInteger ;
                    owl:withRestrictions ( [ xsd:minInclusive "1"^^xsd:positiveInteger
                                           ]
                                           [ xsd:maxInclusive "12"^^xsd:positiveInteger
                                           ]
                                         )
                  ] ;
       rdfs:comment "The month is the month portion of a date." ;
       rdfs:label "month" .


###  http://waysysweb.com/ontolgoy/lootedart#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf owl:topDataProperty ;
      rdf:type owl:FunctionalProperty ;
      rdfs:range xsd:string .


###  http://waysysweb.com/ontolgoy/lootedart#nameFirst
:nameFirst rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:range xsd:string ;
           rdfs:comment "The first name of a person." .


###  http://waysysweb.com/ontolgoy/lootedart#nameLast
:nameLast rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:string ;
          rdfs:comment "The last name of a person." ;
          rdfs:label "nameLast" .


###  http://waysysweb.com/ontolgoy/lootedart#nameMaiden
:nameMaiden rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty .


###  http://waysysweb.com/ontolgoy/lootedart#nameMiddle
:nameMiddle rdf:type owl:DatatypeProperty .


###  http://waysysweb.com/ontolgoy/lootedart#purpose
:purpose rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:range xsd:string .


###  http://waysysweb.com/ontolgoy/lootedart#rank
:rank rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf owl:topDataProperty ;
      rdfs:comment "Military rank" .


###  http://waysysweb.com/ontolgoy/lootedart#title
:title rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf owl:topDataProperty .


###  http://waysysweb.com/ontolgoy/lootedart#year
:year rdf:type owl:DatatypeProperty ;
      rdfs:range [ rdf:type rdfs:Datatype ;
                   owl:onDatatype xsd:positiveInteger ;
                   owl:withRestrictions ( [ xsd:minInclusive "1100"^^xsd:positiveInteger
                                          ]
                                          [ xsd:maxInclusive "2100"^^xsd:positiveInteger
                                          ]
                                        )
                 ] ;
      rdfs:comment "The year is the year portion of a date." ;
      rdfs:label "year" .


###  http://waysysweb.com/ontolgoy/lootedart#yearBirth
:yearBirth rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:onDatatype xsd:positiveInteger ;
                        owl:withRestrictions ( [ xsd:minInclusive "1100"^^xsd:positiveInteger
                                               ]
                                               [ xsd:maxInclusive "2100"^^xsd:positiveInteger
                                               ]
                                             )
                      ] ;
           rdfs:comment "The year a person was born." .


###  http://waysysweb.com/ontolgoy/lootedart#yearDeath
:yearDeath rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:onDatatype xsd:positiveInteger ;
                        owl:withRestrictions ( [ xsd:minInclusive "1100"^^xsd:positiveInteger
                                               ]
                                               [ xsd:maxInclusive "2100"^^xsd:positiveInteger
                                               ]
                                             )
                      ] ;
           rdfs:comment "The year a person died." .


###  http://waysysweb.com/ontolgoy/lootedart#yearDissolved
:yearDissolved rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:range [ rdf:type rdfs:Datatype ;
                            owl:onDatatype xsd:positiveInteger ;
                            owl:withRestrictions ( [ xsd:minInclusive "1100"^^xsd:positiveInteger
                                                   ]
                                                   [ xsd:maxInclusive "2100"^^xsd:positiveInteger
                                                   ]
                                                 )
                          ] ;
               rdfs:comment "The year an organization was disbanded." .


###  http://waysysweb.com/ontolgoy/lootedart#yearFounded
:yearFounded rdf:type owl:DatatypeProperty ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:onDatatype xsd:positiveInteger ;
                          owl:withRestrictions ( [ xsd:minInclusive "1100"^^xsd:positiveInteger
                                                 ]
                                                 [ xsd:maxInclusive "2100"^^xsd:positiveInteger
                                                 ]
                                               )
                        ] ;
             rdfs:comment "The year an organization was organized." .


###  http://www.w3.org/2002/07/owl#topDataProperty
owl:topDataProperty rdfs:comment "The name of the position in an organization." .


#################################################################
#    Classes
#################################################################

###  http://waysysweb.com/ontolgoy/lootedart#Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf :BusinessDealing ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :agentFor ;
                         owl:someValuesFrom :Party
                       ] ;
       rdfs:comment "An agent is a person who acts on behalf of another peron or group." .


###  http://waysysweb.com/ontolgoy/lootedart#ArtCollection
:ArtCollection rdf:type owl:Class ;
               rdfs:subClassOf :ArtWork ;
               rdfs:comment "An art collection is a group of art works that belong to a party." .


###  http://waysysweb.com/ontolgoy/lootedart#ArtDealership
:ArtDealership rdf:type owl:Class ;
               rdfs:subClassOf :Business .


###  http://waysysweb.com/ontolgoy/lootedart#ArtWork
:ArtWork rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :creator ;
                           owl:someValuesFrom :Person
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :name ;
                           owl:someValuesFrom xsd:string
                         ] ;
         rdfs:comment """An item of art work is the expression or application of human creative skill and imagination, typically in a visual form such as painting or sculpture, producing works to be appreciated primarily for their beauty or emotional power.

Art works have a creator.""" .


###  http://waysysweb.com/ontolgoy/lootedart#Auction
:Auction rdf:type owl:Class ;
         rdfs:subClassOf :Event .


###  http://waysysweb.com/ontolgoy/lootedart#Business
:Business rdf:type owl:Class ;
          rdfs:subClassOf :Organization .


###  http://waysysweb.com/ontolgoy/lootedart#BusinessDealing
:BusinessDealing rdf:type owl:Class ;
                 rdfs:subClassOf :Relation ;
                 rdfs:comment "A business dealing is the relation of two parties that transact business over a period of time." .


###  http://waysysweb.com/ontolgoy/lootedart#City
:City rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :Location
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :country ;
                                                   owl:someValuesFrom :Country
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :name ;
                                                   owl:someValuesFrom xsd:string
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:comment "A city is a town, borough, or city in a country." .


###  http://waysysweb.com/ontolgoy/lootedart#Country
:Country rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:oneOf ( :AT
                                           :DE
                                           :FR
                                           :GB
                                           :IT
                                           :LI
                                           :US
                                         )
                             ] ;
         rdfs:subClassOf :Location ;
         rdfs:comment "A country is a geographical location with its own government." .


###  http://waysysweb.com/ontolgoy/lootedart#Duration
:Duration rdf:type owl:Class ;
          rdfs:subClassOf :Timing ;
          rdfs:comment "A duration in a period of time between two point in time." ;
          rdfs:label "Duration" .


###  http://waysysweb.com/ontolgoy/lootedart#Employment
:Employment rdf:type owl:Class ;
            rdfs:subClassOf :Relation ;
            rdfs:comment "Employment is a type of interaction in which a person works for an employer (Party).  Employment has a duration." .


###  http://waysysweb.com/ontolgoy/lootedart#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :atDate ;
                         owl:someValuesFrom :PointInTime
                       ] ;
       rdfs:comment "An event is an occurrance of a thing at a point in time." .


###  http://waysysweb.com/ontolgoy/lootedart#Family
:Family rdf:type owl:Class ;
        rdfs:subClassOf :Party .


###  http://waysysweb.com/ontolgoy/lootedart#GovernmentOrganization
:GovernmentOrganization rdf:type owl:Class ;
                        rdfs:subClassOf :Organization .


###  http://waysysweb.com/ontolgoy/lootedart#Group
:Group rdf:type owl:Class ;
       rdfs:comment "A group is a collection of things." .


###  http://waysysweb.com/ontolgoy/lootedart#Location
:Location rdf:type owl:Class ;
          rdfs:comment "A location is a geographical area." .


###  http://waysysweb.com/ontolgoy/lootedart#MilitaryOrganization
:MilitaryOrganization rdf:type owl:Class ;
                      rdfs:subClassOf :Organization .


###  http://waysysweb.com/ontolgoy/lootedart#Museum
:Museum rdf:type owl:Class ;
        rdfs:subClassOf :Organization .


###  http://waysysweb.com/ontolgoy/lootedart#Network
:Network rdf:type owl:Class ;
         rdfs:subClassOf :Party ;
         rdfs:comment "A network is an informal association of parties that exchange information and transact business over a period of time.  A network may be centered in a particular city." .


###  http://waysysweb.com/ontolgoy/lootedart#Occupation
:Occupation rdf:type owl:Class ;
            rdfs:comment "An occupation is a type of work a person performs.  It may have a duration." .


###  http://waysysweb.com/ontolgoy/lootedart#Operation
:Operation rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Relation
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :conductedBy ;
                                                        owl:someValuesFrom :Party
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:comment "An organizational operation is an activity or project conducted over a period of time by a party." .


###  http://waysysweb.com/ontolgoy/lootedart#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf :Party ;
              rdfs:comment "An organization is a body of persons with a particular purpose." .


###  http://waysysweb.com/ontolgoy/lootedart#Ownership
:Ownership rdf:type owl:Class ;
           rdfs:subClassOf :Relation ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :owner ;
                             owl:someValuesFrom :Party
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :owns ;
                             owl:someValuesFrom :ArtWork
                           ] ;
           rdfs:comment "Ownership is the combination of an owner, a thing that is owned, and a time period of ownership." .


###  http://waysysweb.com/ontolgoy/lootedart#Painting
:Painting rdf:type owl:Class ;
          rdfs:subClassOf :ArtWork .


###  http://waysysweb.com/ontolgoy/lootedart#Partnership
:Partnership rdf:type owl:Class ;
             rdfs:subClassOf :BusinessDealing .


###  http://waysysweb.com/ontolgoy/lootedart#Party
:Party rdf:type owl:Class ;
       rdfs:comment "A party is a person, family, or organization.  A party can participate in a variety of relationships." ;
       rdfs:label "Party" .


###  http://waysysweb.com/ontolgoy/lootedart#Person
:Person rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( :Party
                                                   [ rdf:type owl:Restriction ;
